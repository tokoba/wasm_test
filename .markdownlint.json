{
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "emphasis-style": {
    "style": "asterisk"
  },
  "extended-ascii": {
    "ascii-only": false
  },
  "fenced-code-language": {
    "allowed_languages": [
      "asm",
      "bash",
      "c",
      "c++",
      "cc",
      "cmake",
      "conf",
      "config",
      "cpp",
      "cs",
      "csharp",
      "css",
      "cxx",
      "diff",
      "docker",
      "dockerfile",
      "dotenv",
      "elixir",
      "env",
      "ex",
      "exs",
      "fs",
      "fsharp",
      "go",
      "gql",
      "graphql",
      "h",
      "haskell",
      "hpp",
      "hs",
      "html",
      "ini",
      "java",
      "javascript",
      "json",
      "kotlin",
      "kt",
      "llvm",
      "log",
      "lua",
      "make",
      "makefile",
      "markdown",
      "mermaid",
      "md",
      "ml",
      "ocaml",
      "patch",
      "perl",
      "php",
      "pl",
      "plaintext",
      "powershell",
      "properties",
      "proto",
      "protobuf",
      "ps1",
      "psm1",
      "py",
      "python",
      "r",
      "rb",
      "rs",
      "ruby",
      "rust",
      "s",
      "sass",
      "scala",
      "scss",
      "sh",
      "shell",
      "sql",
      "swift",
      "text",
      "toml",
      "ts",
      "tsx",
      "txt",
      "typescript",
      "xml",
      "yaml",
      "yml",
      "zsh"
    ],
    "language_only": true
  },
  "heading-style": {
    "style": "atx"
  },
  "hr-style": {
    "style": "---"
  },
  "line-length": {
    "line_length": 160,
    "code_block_line_length": 160,
    "heading_line_length": 80,
    "tables": true,
    "headings": true,
    "stern": false,
    "strict": false,
    "code_blocks": false
  },
  "link-image-style": {
    "collapsed": false,
    "shortcut": false,
    "url_inline": false
  },
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "ol-prefix": {
    "style": "ordered"
  },
  "proper-names": {
    "code_blocks": false,
    "names": [
      "Cake.Markdownlint",
      "CommonMark",
      "JavaScript",
      "Markdown",
      "markdown-it",
      "markdownlint",
      "Node.js"
    ]
  },
  "reference-links-images": {
    "shortcut_syntax": true
  },
  "strong-style": {
    "style": "asterisk"
  },
  "ul-style": {
    "style": "dash"
  }
}
